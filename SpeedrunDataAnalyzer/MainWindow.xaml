<Window x:Class="SpeedrunDataAnalyzer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpeedrunDataAnalyzer"
        mc:Ignorable="d"
        Title="Speedrun Data Analyzer" Height="450" Width="800">
    <Grid Background="#FF1A3E56">
        <TextBlock x:Name="TimeTextBox" HorizontalAlignment="Left" Margin="16,53,0,0" TextWrapping="Wrap" Text="{Binding RunID}" VerticalAlignment="Top" Height="23" Width="196" Background="White"/>
        <Button Content="Start/Pause" HorizontalAlignment="Left" Margin="83,112,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <Button Content="Lap" HorizontalAlignment="Left" Margin="103,148,0,0" VerticalAlignment="Top"/>
        <Button Content="Reset" HorizontalAlignment="Left" Margin="98,186,0,0" VerticalAlignment="Top"/>
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="35,217,607,30"/>

    </Grid>
</Window>
